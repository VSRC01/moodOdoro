; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MoodOdoro"
config/features=PackedStringArray("4.5", "Forward Plus")
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

UIAnimation="*res://addons/godot_ui_animations/UIAnimationHandler.tscn"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"

[display]

window/size/viewport_width=600
window/size/viewport_height=1000
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-rpc-gd/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg", "res://addons/godot_ui_animations/plugin.cfg", "res://addons/ridiculous_coding/plugin.cfg")

[input_devices]

pointing/android/enable_pan_and_scale_gestures=true
pointing/emulate_mouse_from_touch=false

[rendering]

renderer/rendering_method="mobile"
